<Window x:Class="CardCollectionGUI.RemoveCardWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CardCollectionGUI"
        mc:Ignorable="d"
        Title="RemoveCardWindow" Height="150" Width="300" Background="Azure" ResizeMode="NoResize">
    <Grid HorizontalAlignment="Left" Width="294">
        <Button x:Name="button" Content="Remove Card" Width="110" Margin="113,55,71,0" VerticalAlignment="Top" Click="remove_button"/>
        <Label x:Name="label1" Content="Enter SerialNumber: " HorizontalAlignment="Left" Margin="10,18,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="140,20,0,0" Width="60" TextWrapping="Wrap" Text="" VerticalAlignment="Top" TextChanged="textBox_TextChanged"/>

    </Grid>
</Window>

<Window x:Class="CardCollectionGUI.CardCreationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CardCollectionGUI"
        mc:Ignorable="d"
        Title="Card Creation" Height="300" Width="300" Background="Azure">
    <Grid>
        <Button x:Name="binderbutton" Content="" Width="Auto" Height="Auto" Margin="150,130,20,20" Click="binderbutton_Click">
            <Button.Background>
                <ImageBrush ImageSource="Images/Disneybog.jpg"></ImageBrush>
            </Button.Background>
        </Button>
    </Grid>
</Window>
